{
  "name": "taxrates-us",
  "version": "0.4.0",
  "description": "Accurate US sales tax rate lookups - All 50 states + DC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "next dev",
    "build": "next build && npm run build:package",
    "start": "next start",
    "build:package": "tsc -p tsconfig.package.json",
    "test": "tsx security-tests/test-bad-data.ts && node --test dist/**/*.test.js",
    "test:security": "tsx security-tests/test-bad-data.ts",
    "validate:data": "tsx scripts/auto-update/validate-staged.ts",
    "prepublishOnly": "npm run build:package && npm test",
    "lint": "eslint"
  },
  "keywords": [
    "tax",
    "sales-tax",
    "california",
    "tax-rates",
    "cdtfa",
    "taxation"
  ],
  "author": "mrpartner",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrpartnerai/taxrates-us.git"
  },
  "bugs": {
    "url": "https://github.com/mrpartnerai/taxrates-us/issues"
  },
  "homepage": "https://github.com/mrpartnerai/taxrates-us#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^22.0.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vercel/node": "^3.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "typescript": "^5.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
